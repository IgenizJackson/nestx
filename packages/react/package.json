{
  "name": "nest-react",
  "version": "0.2.0-alpha.0",
  "description": "react client side",
  "scripts": {
    "presite": "cd functions && npm install",
    "start": "cross-env APP_TYPE=site umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "dev": "cross-env APP_TYPE=site umi dev",
    "dev:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint": "umi-lint --prettier --stylelint src/",
    "lint:style": "umi-lint --stylelint src/",
    "lint:prettier": "umi-lint --prettier src/",
    "lint:ts": "umi-lint --eslint.ext='.ts,.tsx' src/",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js",
    "pre-commit": "npm run lint"
  },
  "dependencies": {
    "ant-design-pro": "^2.1.1",
    "antd": "3.15.1",
    "qs": "^6.6.0",
    "classnames": "^2.2.6",
    "dva": "^2.4.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.22.2",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "rc-animate": "^2.4.4",
    "react": "^16.6.3",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-media-hook2": "^1.0.2",
    "umi-request": "^1.0.0",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.122",
    "@types/memoize-one": "^4.1.0",
    "@types/qs": "^6.5.2",
    "@types/react": "^16.8.1",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.0.11",
    "antd-pro-merge-less": "^1.0.0",
    "antd-theme-webpack-plugin": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.1",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.9.0",
    "eslint-config-umi": "^2.0.0-beta.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest-puppeteer": "^4.0.0",
    "less": "^3.9.0",
    "merge-umi-mock-data": "^1.0.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.16.4",
    "slash2": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-order": "^2.0.0",
    "umi": "^2.4.4",
    "umi-lint": "1.0.0",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-pro-block": "^1.2.0",
    "umi-plugin-react": "^1.3.4",
    "umi-types": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vellengs/nestx.git"
  },
  "bugs": {
    "url": "https://github.com/vellengs/nestx/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "homepage": "https://github.com/vellengs/nestx#readme",
  "keywords": [],
